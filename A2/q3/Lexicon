% past tense for Trans Verbs
V_p_trans -> 'was' | 'had' | 'jumped' | 'fondled' | 'brought' | 'reminded' | 'rewarded' | 'found' | 'helped' | 'left' | 'shot' | 'ate' | 'told' | 'believed' | 'won' | 'saw' | 'wanted' | 'helped' | 'demanded' | 'gave'

V_p_intrans -> 'fondled' | 'was' | 'arrived' | 'helped' | 'left' | 'shot' | 'ate' | 'jumped' | 'believed' | 'won' | 'saw' | 'wanted' | 'helped' | 'demanded' | 'gave' |'aspired'  
V_pre_intrans -> 'fondle' | 'be' | 'arrive' | 'help' | 'leave' | 'shoot' | 'eat' | 'jump' | 'believe' | 'win' | 'see' | 'want' | 'help' | 'demand' | 'give' | 'aspire'
V_pre_trans -> 'has' | 'fondle' | 'bring' | 'remind' | 'reward' | 'find' | 'help' | 'leave' | 'shoot' | 'eat' | 'tell' | 'believe' | 'win' | 'see' | 'want' | 'help' | 'demand' | 'give'

% trans verbs could followed by that
V_p_trans_that -> 'found' | 'believed' | 'saw' | 'demanded' 

% trans verbs could followed by object then P
V_p_trans_np_that -> 'reminded' | 'told' | 'believed'

% intranstive verbs that must followed by P
V_p_intrans_musthavep_p -> 'was' | 'wanted' | 'demanded' 

% intransitive verbs that followed by before or after
V_p_intrans_con -> 'ate' | 'arrived' | 'left' | 'shot' | 'jumped' | 'won'


% Passive
Passive_simple -> 'was'
